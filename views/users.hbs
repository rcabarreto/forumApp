
<ol class="breadcrumb">
  <li><a href="/"><span class="glyphicon glyphicon-home"></span></a></li>
  <li class="active">User management</li>
</ol>


<div class="page-header">
  <div class="row">
    <div class="col-xs-12"><h1>User management</h1></div>
  </div>
</div>


<table class="table">
  <caption>List of all users</caption>
  <thead>
  <tr>
    <th>#</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Display Name</th>
    <th>Email</th>
    <th>Profile</th>
  </tr>
  </thead>
  <tbody>

  {{#each users as |user|}}
    <tr>
      <th scope="row">{{user.id}}</th>
      <td>{{user.first_name}}</td>
      <td>{{user.last_name}}</td>
      <td>{{user.display_name}}</td>
      <td>{{user.email}}</td>
      <td>{{user.profile}}</td>
    </tr>
  {{/each}}

  </tbody>
</table>
