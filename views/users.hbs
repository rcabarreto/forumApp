
<table class="table">
  <caption>Optional table caption.</caption>
  <thead>
  <tr>
    <th>#</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Display Name</th>
    <th>Email</th>
    <th>Profile</th>
  </tr>
  </thead>
  <tbody>

  {{#each users as |user|}}
    <tr>
      <th scope="row">1</th>
      <td>{{user.first_name}}</td>
      <td>{{user.last_name}}</td>
      <td>{{user.display_name}}</td>
      <td>{{user.email}}</td>
      <td>{{user.profile}}</td>
    </tr>
  {{/each}}

  </tbody>
</table>
